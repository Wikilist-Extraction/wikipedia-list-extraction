io {
  inputFile = "data/input.json"
  jsonOutput = "results/output.json"
  rdfMembershipOutput = "results/membership.ttl"
  rdfTypesOutput = "results/types.ttl"
}

leacock {
  forEachThreshold = -2.5
  meanThreshold = -2
  useMeanThreshold = false
}

fusing {
  weights {
    tfIdf = 1
    textEvidence = 1
  }
}

filtering {
  strategy = "threshold"
  thresholds {
    final = 0.7
    allTypesCorrect = 0
  }
}